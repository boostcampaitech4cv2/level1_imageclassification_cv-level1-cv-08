{
    "name": "Mask_base",
    "n_gpu": 1,
    "arch": {
        "type": "TimmModel",
        "args": {
            "model_name": "efficientnetv2_rw_s",
            "pretrained": true,
            "num_classes": 18
        }
    },
    "data_loader": {
        "type": "setup",
        "args": {
            "stage": "train",
            "input_size": 288,
            "batch_size": 64,
            "num_workers": 4
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "ce_loss",
    "metrics": [],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 5,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val/loss",
        "early_stop": 5,
        "tensorboard": false
    },
    "wandb": true,
    "submit_dir": "/opt/ml/level1_imageclassification_cv-level1-cv-08/submit",
    "info_dir": "/opt/ml/input/data/eval"
}